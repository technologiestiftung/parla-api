{
	"name": "@technologiestiftung/parla-api",
	"version": "1.19.0",
	"description": "",
	"main": "dist/index.js",
	"type": "module",
	"scripts": {
		"test": "NODE_OPTIONS='--no-warnings' c8 ava",
		"dev": "nodemon",
		"dev:cors": "NODE_ENV=none nodemon",
		"build": "mkdir -p dist && node esbuild.config.cjs",
		"start": "node dist/index.js",
		"create:types": "supabase gen types typescript --local > ./src/lib/database.ts && prettier --write ./src/lib/database.ts"
	},
	"keywords": [],
	"author": "ff6347 <hash@ff6347.email>",
	"license": "MIT",
	"devDependencies": {
		"@ava/typescript": "4.1.0",
		"@technologiestiftung/semantic-release-config": "1.2.4",
		"@types/common-tags": "1.8.4",
		"@types/node": "20.9.4",
		"@typescript-eslint/eslint-plugin": "6.12.0",
		"@typescript-eslint/parser": "6.12.0",
		"ava": "6.0.1",
		"c8": "8.0.1",
		"cz-conventional-changelog": "3.3.0",
		"dotenv": "16.3.1",
		"esbuild": "0.19.7",
		"eslint": "8.54.0",
		"eslint-config-prettier": "9.0.0",
		"eslint-plugin-prettier": "5.0.1",
		"glob": "10.3.10",
		"msw": "2.0.9",
		"nodemon": "3.0.1",
		"pino-pretty": "10.2.3",
		"prettier": "3.1.0",
		"semantic-release": "22.0.8",
		"supabase": "1.113.2",
		"tsx": "4.6.2",
		"typescript": "5.2.2"
	},
	"prettier": {
		"tabWidth": 2,
		"semi": true,
		"singleQuote": false,
		"useTabs": true,
		"printWidth": 80
	},
	"nodemonConfig": {
		"restartable": "rs",
		"verbose": true,
		"execMap": {
			"js": "node --harmony"
		},
		"exec": "tsx ./src/index.ts",
		"watch": [
			"src"
		],
		"env": {
			"NODE_ENV": "development"
		},
		"ext": "ts,json"
	},
	"dependencies": {
		"@fastify/cors": "8.3.0",
		"@fastify/rate-limit": "9.1.0",
		"@fastify/swagger": "8.10.0",
		"@fastify/swagger-ui": "1.9.3",
		"@supabase/supabase-js": "2.39.0",
		"common-tags": "1.8.2",
		"fastify": "4.22.2",
		"fluent-json-schema": "4.1.2",
		"gpt3-tokenizer": "1.1.5",
		"openai": "4.24.1",
		"react-device-detect": "2.2.3"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
